						<resource id="Lof_AdvancedReports::salesbydayofweek" title="Sales By Day Of Week" sortOrder="20" />
						<resource id="Lof_AdvancedReports::salesbyproduct" title="Sales By Product" sortOrder="20" />
						<resource id="Lof_AdvancedReports::salesbycategory" title="Sales By Category" sortOrder="20" />
