						<resource id="Lof_AdvancedReports::topcustomers" title="Top Customers Reports" sortOrder="20" />
						<resource id="Lof_AdvancedReports::customerscity" title="Customer City" sortOrder="20" />
						<resource id="Lof_AdvancedReports::customerscountry" title="Customer Country" sortOrder="20" />
