					<resource id="Lof_AdvancedReports::testcron" title="Test Cron" sortOrder="20">
						<resource id="Lof_AdvancedReports::testcronrun" title="Allow force run cron job" sortOrder="10" />
					</resource>
				</resource>
				<resource id="Magento_Backend::stores">
					<resource id="Magento_Backend::stores_settings">
						<resource id="Magento_Config::config">
							<resource id="Lof_AdvancedReports::config_reports" title="Config Lof Advanced Reports" />
						</resource>
					</resource>
				</resource>
			</resource>
		</resources>
	</acl>
