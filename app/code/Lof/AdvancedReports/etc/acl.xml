<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="Lof_AdvancedReports::advancedreport" title="Lof Advanced Reports">
					<resource id="Lof_AdvancedReports::earningreport" title="Earnings Reports" sortOrder="10">
						<resource id="Lof_AdvancedReports::earningreport_view" title="View Earning Reports" sortOrder="10" />
						<resource id="Lof_AdvancedReports::earningreport_export" title="Export Earning Reports" sortOrder="20" />
					</resource>
					<resource id="Lof_AdvancedReports::order" title="Sales Statists" sortOrder="10">
						<resource id="Lof_AdvancedReports::orderdetailed" title="View Order Detail" sortOrder="10" />
						<resource id="Lof_AdvancedReports::orderitemsdetailed" title="View Order Items Detail" sortOrder="20" />
