<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <googletagmanager>
            <general>
                <enabled>1</enabled>
            </general>
            <googletag>
                <enabled>0</enabled>
                <use_id_or_sku>1</use_id_or_sku>
            </googletag>
            <analyticstag>
                <enabled>0</enabled>
                <use_id_or_sku>1</use_id_or_sku>
                <link_attribution>1</link_attribution>
                <linker>0</linker>
                <display_features>1</display_features>
                <ip_anonymization>0</ip_anonymization>
            </analyticstag>
            <pixeltag>
                <enabled>0</enabled>
                <use_id_or_sku>1</use_id_or_sku>
            </pixeltag>
        </googletagmanager>
    </default>
</config>