<script type="text/javascript">
   
    jQuery('.tyro_action_icon').click(function(){
        var tabcontent=jQuery(this).find('.nav-item').attr('aria-controls');

        jQuery('.tyro_action_icon').not(this).each(function(){
                 jQuery(this).removeClass('tabselected');
             });

        jQuery('.tab-pane').not(jQuery('#'+tabcontent)).each(function(){
             jQuery(this).removeClass('in selectetabcontent');
             jQuery(this).hide();
         });

        jQuery(this).removeClass('active');

        jQuery(this).toggleClass('tabselected');

        jQuery('#'+tabcontent).toggleClass('in selectetabcontent');

        if(jQuery('#'+tabcontent).is(':hidden') ){ console.log('yes');
            jQuery('#'+tabcontent).show();
        }else{
            jQuery('#'+tabcontent).hide();
        }
  });
</script>
